import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		
		System.out.println("             MULTIPLICATION TABLE");
		System.out.println();
		
		
		for (int top = 0; top<=10; top++){
			System.out.printf("%3d ",top);
		}
		System.out.println(" ");
		
		
		for(int i=1; i<=10; i++){
			System.out.printf("%3d ",i);
			for(int j= 1; j<=10; j++){
				System.out.printf("%3d ",i*j);
			}
			System.out.println(" ");
		}
			
    }
   	
	
}
